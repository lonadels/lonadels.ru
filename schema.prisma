datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ProxyKey {
  id        Int      @id @default(autoincrement())
  accessUrl String   @unique @map("access_url")
  createdAt DateTime @default(now()) @map("created_at")

  @@map("proxy_keys")
}
