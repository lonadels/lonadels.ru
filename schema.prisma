datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ProxyKey {
  uuid      String    @id @default(uuid())
  id        String    @unique
  accessUrl String    @unique @map("access_url")
  createdAt DateTime  @default(now()) @map("created_at")
  devices   Devices[]

  @@index([id])
  @@map("proxy_keys")
}

model Devices {
  id   Int        @id @default(autoincrement())
  ip   String     @unique
  keys ProxyKey[]

  @@map("devices")
}
